				 maxEvents: -1
}

services : @local::Services.Reco

physics:
{

		producers : {
				@table::TrkHitReco.producers
		}

    analyzers : {
        CHD : {
          module_type : TracksOutputSQL
					StrawDigiMCCollection : "compressDigiMCs"
          ComboHitCollection : "makeSH"
