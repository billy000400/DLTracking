process_name : TracksToSQL

source:{ module_type : RootInput
				 fileNames : ["/pnfs/mu2e/tape/phy-sim/dig/mu2e/flatmugamma-mix/MDC2018d/art/00/0c/dig.mu2e.flatmugamma-mix.MDC2018d.001002_00003528.art"]
